<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.astuetz.viewpager.extensions.sample">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ball15"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme">
        <activity
            android:name="com.astuetz.viewpager.extensions.gramis.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.Task1Activity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.completed.CompletedActivity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.about_icon.AboutIconActivity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.profile.ProfileActivity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.rating.RatingActivity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.active.ActiveActivity"></activity>
        <activity android:name="com.astuetz.viewpager.extensions.gramis.icons.IconsActivity"></activity>
    </application>

</manifest>